local function webhookmessage(data1, link)
    local HttpService = game:GetService("HttpService")

    local response = http_request({
        Url = link,

        Method = "POST",
        Headers = {
            ["Content-Type"] = "application/json"
        },

        Body = HttpService:JSONEncode(data1)
    })

    print(response.Success)
end

