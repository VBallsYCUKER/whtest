-- Create the base "i" using parts
local function createI()
    -- Create the bottom part of the "i"
    local bottomPart = Instance.new("Part")
    bottomPart.Size = Vector3.new(1, 0.5, 1)  -- Adjust size as needed
    bottomPart.Position = Vector3.new(0, 1, 0)  -- Set the position
    bottomPart.Anchored = true
    bottomPart.BrickColor = BrickColor.new("White") -- Set the color
    bottomPart.Parent = workspace

    -- Create the vertical part of the "i"
    local middlePart = Instance.new("Part")
    middlePart.Size = Vector3.new(0.5, 3, 0.5)  -- Thin vertical part
    middlePart.Position = Vector3.new(0, 3, 0)  -- Position above the bottom part
    middlePart.Anchored = true
    middlePart.BrickColor = BrickColor.new("White")
    middlePart.Parent = workspace

    -- Create the top part of the "i" (dot)
    local topPart = Instance.new("Part")
    topPart.Size = Vector3.new(0.5, 0.5, 0.5)  -- Small square for the dot
    topPart.Position = Vector3.new(0, 5, 0)  -- Position above the vertical part
    topPart.Anchored = true
    topPart.BrickColor = BrickColor.new("White")
    topPart.Parent = workspace
end

-- Call the function to create the "i"
createI()
