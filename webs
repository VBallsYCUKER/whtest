𝒾𝒹𝑒𝒶 — Today at 8:44 PM
using UnityEngine;

public class TeleportOnce : MonoBehaviour
{
    // Set the target coordinates in the Inspector or directly in the script
    private Vector3 targetPosition = new Vector3(-41425.7f, 86491.1f, -29050.5f);
    private bool hasTeleported = false;

    void Update()
    {
        // Check if the teleportation has already been performed
        if (!hasTeleported)
        {
            Teleport();
        }
    }

    void Teleport()
    {
        // Teleport the player or object to the target position
        transform.position = targetPosition;
        hasTeleported = true; // Ensure this only happens once
        Debug.Log("Teleported to: " + targetPosition);
    }
}
